{
  "name": "assistant",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eas-updates-preview": "dotenv -e .env -- eas update --branch preview --message \"Preview app updates\"",
    "eas-build-preview-ios": "dotenv -e .env -- eas build --profile preview --platform ios",
    "eas-updates-production": "dotenv -e .env.prod -- eas update --branch production --message \"Production app updates\"",
    "eas-build-production-ios": "dotenv -e .env.prod -- eas build --profile production --platform ios",
    "eas-build-on-success": "npx bugsnag-eas-build-on-success"
  },
  "dependencies": {
    "@bugsnag/expo": "~48.1.0",
    "@bugsnag/react-native": "^7.20.2",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/netinfo": "9.3.7",
    "@react-native-community/slider": "^4.4.2",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "axios": "^1.4.0",
    "dotenv": "16.3.0",
    "dotenv-cli": "^7.2.1",
    "expo": "~48.0.18",
    "expo-application": "~5.1.1",
    "expo-constants": "~14.2.1",
    "expo-crypto": "~12.2.1",
    "expo-device": "~5.2.1",
